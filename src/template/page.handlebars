<div class="header">
    <div class="top">
        <span class="title">{{title}}</span>
        <div class="labels">
            <span class="id">{{id}}</span>
            <span class="url"><a target="_blank" href={{url}}>full url</a></span>
            <span class="url"><a target="_blank" href={{editUrl}}>edit url</a></span>
        </div>
    </div>
    <span class="search">result of:
        <span class="search-term">{{searchPhrase}}</span>
    </span>
</div>

<div class="content">
    <div class="description">
        {{description}}
    </div>
    <div class="extract">
        {{{summary}}}
    </div>
</div>

<div class="links">
    <button class="extlinks-button">External Links</button>
    <div class="extlinks">
        {{#each extlinks}}
            <a href={{this}}>{{this}}</a>
        {{/each}}
    </div>
</div>

<hr>
{
  "name": "wikichan",
  "version": "3.0.0",
  "description": "A WebExtension and userscript to show an info popup on hovering words or phrases.",
  "repository": "https://github.com/Dophin2009/wikichan.git",
  "homepage": "https://github.com/Dophin2009/wikichan",
  "author": "Eric Zhao",
  "license": "GPL-3.0",
  "scripts": {
    "dev:ujs": "yarn dev --env target=ujs",
    "dev:firefox": "yarn dev --env target=firefox",
    "dev:chrome": "yarn dev --env target=chrome",
    "dev:opera": "  yarn dev --env target=opera",
    "dev:edge": "yarn dev --env target=edge",
    "dev": "yarn build --env production=false",
    "start:ujs": "yarn start --env target=ujs",
    "start:firefox": "yarn start --env target=firefox",
    "start:chrome": "yarn start --env target=chrome",
    "start:opera": "  yarn start --env target=opera",
    "start:edge": "yarn start --env target=edge",
    "start": "cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" webpack serve --env production=false",
    "build:ujs": "yarn dev --env target=ujs",
    "build:firefox": "yarn build --env target=firefox",
    "build:chrome": "yarn build --env target=chrome",
    "build:opera": "yarn build --env target=opera",
    "build:edge": "yarn build --env target=edge",
    "build": "cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" webpack"
  },
  "devDependencies": {
